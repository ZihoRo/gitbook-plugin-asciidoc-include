{
    "name": "gitbook-plugin-asciidoc-include",
    "description": "Syntax asciidoc include for Gitbook",
    "version": "0.1.6",
    "engines": {
      "gitbook": ">=2.4.0"
    },
    "homepage": "https://github.com/ZihoRo/gitbook-plugin-asciidoc-include",
    "repository": {
      "type": "git",
      "url": "https://github.com/ZihoRo/gitbook-plugin-asciidoc-include.git"
    },
    "bugs": {
      "url": "https://github.com/ZihoRo/gitbook-plugin-asciidoc-include/issues"
    },
    "gitbook": {
        "properties": {
            "charset": {
                "type": "string",
                "default": "UTF-8",
                "description": "include file encoding"
            }
        }
    },
    "dependencies": {
        "q": "1.4.1"
    },
    "keywords": [
        "gitbook",
        "plugin",
        "asciidoc",
        "include"
    ],
    "license": "MIT",
    "author": "ZihoRo <zihoro@zihoro.com>"
}
